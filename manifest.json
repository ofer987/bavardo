{
  "name": "Bravado",
  "description": "Add keyboard shortcuts Microsoft Outlook",
  "version": "0.1",
  "icons": {
    "64": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "persistent": false,
    "scripts": ["setCommands.js"]
  },
  "commands": {
    "open-categories": {
      "suggested_key": {
        "default": "Alt+L"
      },
      "description": "Open the Categories Menu"
    }
  },
  "manifest_version": 2
}
